{
  "name": "effect-kv",
  "version": "2.0.0",
  "description": "Lightweight, type-safe wrapper around Cloudflare KV using Effect.ts",
  "author": "Jordi Bermejo",
  "license": "MIT",
  "packageManager": "pnpm@10.23.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/effect-kv.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/effect-kv/issues"
  },
  "homepage": "https://github.com/yourusername/effect-kv#readme",
  "keywords": [
    "effect",
    "effect-ts",
    "cloudflare",
    "kv",
    "key-value",
    "typescript",
    "functional",
    "wrangler",
    "workers"
  ],
  "files": ["dist", "LICENSE", "README.md"],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm run build && pnpm run test && pnpm run typecheck",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "lint": "pnpm run typecheck && pnpm run format:check"
  },
  "peerDependencies": {
    "effect": "^3.0.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.12.8",
    "@cloudflare/workers-types": "^4.20260130.0",
    "prettier": "^3.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
